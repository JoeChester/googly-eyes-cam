<?xml version='1.0' encoding='utf-8'?>
<widget id="de.crossoak.googlyeyes" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="CameraPreview">
        <param name="android-package" value="com.cordovaplugincamerapreview.CameraPreview" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Canvas2ImagePlugin">
        <param name="android-package" value="org.devgeeks.Canvas2ImagePlugin.Canvas2ImagePlugin" />
    </feature>
    <allow-intent href="market:*" />
    <name>Googly Eyes Cam</name>
    <description>
        Augmented Reality Eye Tracking App
    </description>
    <author email="jonas.kleinkauf@informatik.hs-fulda.de" href="http://www.crossoak.de">
        Jonas Kleinkauf
    </author>
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <content src="settings.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
